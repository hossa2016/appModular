<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

<application>
    <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
</application>


<managed-bean>
    <managed-bean-name>dominioUsuarios</managed-bean-name>
    <managed-bean-class>com.dominio.obusiness.dominio.TsEmpUsuarios</managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
</managed-bean>

<managed-bean>
    <managed-bean-name>loginBB</managed-bean-name>
    <managed-bean-class>com.web.obusiness.bean.LoginBB</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    <managed-property>
        <property-name>usuario</property-name>
        <value>#{dominioUsuarios}</value>
    </managed-property>
    <managed-property>
        <property-name>loginService</property-name>
        <value>#{loginService}</value>
    </managed-property>
</managed-bean>


</faces-config>
